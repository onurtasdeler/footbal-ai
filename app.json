{
  "expo": {
    "name": "Goalwise",
    "slug": "goalwise",
    "owner": "goalwin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "extra": {
      "revenueCatApiKeyIos": "appl_LEpIMzKcSrPWBQHQnWJXzfykDJR",
      "revenueCatApiKeyAndroid": "goog_NDPzwjjWBkOLcHsWRYtrXoEIEZx",
      "eas": {
        "projectId": "d43bf147-1caf-4d73-a3e6-d07be6e6c13f"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goalwise.epinko",
      "buildNumber": "10",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": [],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0e13"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.goalwise.epinko",
      "versionCode": 12,
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0a0e13"
      },
      "blockedPermissions": [
        "com.google.android.gms.permission.AD_ID",
        "android.permission.ACCESS_ADSERVICES_AD_ID",
        "android.permission.ACCESS_ADSERVICES_ATTRIBUTION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      "./plugins/withDisableAdId.js",
      "./plugins/withExcludeAdsIdentifier.js"
    ]
  }
}
