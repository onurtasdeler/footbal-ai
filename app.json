{
  "expo": {
    "name": "Goalwise",
    "slug": "goalwise",
    "owner": "goalwin",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "extra": {
      "revenueCatApiKeyIos": "YOUR_REVENUECAT_IOS_KEY",
      "revenueCatApiKeyAndroid": "YOUR_REVENUECAT_ANDROID_KEY",
      "supabaseUrl": "YOUR_SUPABASE_URL",
      "supabaseAnonKey": "YOUR_SUPABASE_ANON_KEY",
      "eas": {
        "projectId": "YOUR_EAS_PROJECT_ID"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goalwise.epinko",
      "buildNumber": "20",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": [],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0e13"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.goalwise.epinko",
      "versionCode": 23,
      "googleServicesFile": "./google-services.json",
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0a0e13"
      },
      "blockedPermissions": [
        "com.google.android.gms.permission.AD_ID",
        "android.permission.ACCESS_ADSERVICES_AD_ID",
        "android.permission.ACCESS_ADSERVICES_ATTRIBUTION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "expo-localization",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "./plugins/withDisableAdId.js",
      "./plugins/withExcludeAdsIdentifier.js",
      "./plugins/withFirebaseModularHeaders.js"
    ]
  }
}
